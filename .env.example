# Configuração do GitHub
GITHUB_TOKEN=ghp_your_github_personal_access_token_here
GITHUB_REPO=manga-index
GITHUB_USERNAME=your_github_username

# Configuração do ImgBB
IMGBB_API_KEY=your_imgbb_api_key_here

# Configuração do Imgur
IMGUR_CLIENT_ID=your_imgur_client_id_here

# Configuração do Catbox (opcional)
CATBOX_USER_HASH=your_catbox_user_hash_here

# Configurações opcionais de desenvolvimento
NODE_ENV=development
DEBUG=true

# Configurações de rate limiting (opcional)
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=60000

# Configurações de upload (opcional)
MAX_CONCURRENT_UPLOADS=3
RETRY_ATTEMPTS=3
RETRY_DELAY=1000

# Configurações de cache (opcional)
CACHE_DURATION=300000